(executable
 ((name network_test)
  (flags (:standard -bin-annot))
  (libraries (astring
              networklibs
              oUnit
              profiling
              xapi-test-utils)
  )
 )
)

(alias
 ((name    runtest)
  (deps    (network_test.exe))
  (action  (chdir ../../.. (run ${<})))))
